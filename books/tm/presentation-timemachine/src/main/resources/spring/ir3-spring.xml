<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.springframework.org/schema/beans" xmlns:p="http://www.springframework.org/schema/p"
  xmlns:util="http://www.springframework.org/schema/util" xmlns:c="http://www.springframework.org/schema/c"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
        http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd">

  <util:constant static-field="hu.zagor.gamebooks.support.bookids.hungarian.IdogepRegeny.A_SARKANYGYIKOK_NYOMABAN" id="ir3Id" />

  <bean class="hu.zagor.gamebooks.domain.BookInformations" id="ir3Info" c:id-ref="ir3Id" p:resourceDir="tm2"
    p:contentTransformers-ref="defaultTmContentTransformers" p:paragraphResolver-ref="tmRuleBookParagraphResolver" p:locale-ref="huLocale" p:position="3" p:series-ref="irTitle" p:title="A sárkánygyíkok nyomában" p:characterHandler-ref="tmCharacterHandler">
    <property name="contents">
      <bean class="hu.zagor.gamebooks.domain.BookContentFiles" c:enemies="" c:items="ir3items.xml" c:paragraphs="ir3content.xml" />
    </property>
    <property name="contentSpecification">
      <bean class="hu.zagor.gamebooks.domain.BookContentSpecification" p:inventoryAvailable="true" p:mapAvailable="false" />
    </property>
  </bean>

</beans>
