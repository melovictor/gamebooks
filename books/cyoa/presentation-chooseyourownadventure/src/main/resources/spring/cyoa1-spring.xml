<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.springframework.org/schema/beans" xmlns:p="http://www.springframework.org/schema/p"
  xmlns:util="http://www.springframework.org/schema/util" xmlns:c="http://www.springframework.org/schema/c"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd">

  <util:constant static-field="hu.zagor.gamebooks.support.bookids.english.ChooseYourOwnAdventure.THE_CAVE_OF_TIME" id="cyoa1Id" />

  <bean class="hu.zagor.gamebooks.domain.BookInformations" id="cyoa1Info" c:id-ref="cyoa1Id" p:coverPath="resources/cyoa1/cover.jpg"
    p:contentTransformers-ref="defaultRawContentTransformers" p:paragraphResolver-ref="rawRuleBookParagraphResolver" p:locale-ref="enLocale" p:position="1" p:series-ref="cyoaTitle" p:title="The Cave of Time" p:characterHandler-ref="rawCharacterHandler">
    <property name="contents">
      <bean class="hu.zagor.gamebooks.domain.BookContentFiles" c:enemies="" c:items="" c:paragraphs="cyoa1content.xml" />
    </property>
    <property name="contentSpecification">
      <bean class="hu.zagor.gamebooks.domain.BookContentSpecification" />
    </property>
  </bean>

</beans>
