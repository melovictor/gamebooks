<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.springframework.org/schema/beans" xmlns:p="http://www.springframework.org/schema/p"
  xmlns:util="http://www.springframework.org/schema/util" xmlns:c="http://www.springframework.org/schema/c"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
        http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd">

  <util:constant static-field="hu.zagor.gamebooks.support.bookids.english.FightingFantasy.TEMPLE_OF_TERROR" id="ff14Id" />

  <bean class="hu.zagor.gamebooks.domain.FfBookInformations" id="ff14Info" c:id-ref="ff14Id" p:coverPath="resources/ff14/cover.jpg" p:resourceDir="ff14"
    p:contentTransformers-ref="defaultFfContentTransformers" p:paragraphResolver-ref="ffRuleBookParagraphResolverLe" p:locale-ref="enLocale" p:position="14" p:series-ref="ffTitle" p:title="Temple of Terror" p:characterHandler-ref="ffCharacterHandler" p:commandResolvers-ref="ffCommandResolvers" p:unfinished="true" p:hidden="true">
    <property name="contents">
      <bean class="hu.zagor.gamebooks.domain.BookContentFiles" c:enemies="ff14enemies.xml" c:items="ff14items.xml" c:paragraphs="ff14content.xml" />
    </property>
    <property name="contentSpecification">
      <bean class="hu.zagor.gamebooks.domain.BookContentSpecification" p:inventoryAvailable="true" p:mapAvailable="false" />
    </property>
  </bean>

</beans>
